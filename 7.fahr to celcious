#include <stdio.h>

int main() {
    int fahr,celsious;
    int low,upp,step;
    low=0;
    upp=300;
    step=20;
    fahr=low;
    while(fahr<=upp)
    {
        celsious=5*(fahr-32/9);
        printf("%d\t%d\n",fahr,celsious);
        fahr=fahr+step;
    }


	return 0;
}


