#include <stdio.h>

int main() {
    float fahr,celsious;
    int low,upp,step;
    low=0;
    upp=300;
    step=20;
    fahr=low;
    while(fahr<=upp)
    {
        celsious=(fahr-32.0)*5.0/9.0;
        printf("%3.0f\t%6.1f\n",fahr,celsious);
        fahr=fahr+step;
    }


	return 0;
}




